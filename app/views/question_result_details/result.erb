
<script type="text/javascript">
$(function(){
    line1 =<%=raw @data.inspect %>
     
     //--添加横纵坐标分类
     plot = $.jqplot('chart', [line1], {

     title: '问卷结果统计图',
         seriesDefaults: {
         renderer: $.jqplot.BarRenderer, //使用柱状图表示
          pointLabels: { show: true },
         rendererOptions: {
              barMargin: 60   //柱状体组之间间隔
          }
         },
         axes: {
             xaxis: {
                 renderer: $.jqplot.CategoryAxisRenderer, //x轴绘制方式
                 label: '问卷大类',
           labelRenderer: $.jqplot.CanvasAxisLabelRenderer
             },
             yaxis: {
             min: 0,           //y轴最小值
             //max: 650,          //y轴最大值
             numberTicks:6,           //网格线条数
             tickInterval: 20,        //网格线间隔大小
             label: '分数',
           labelRenderer: $.jqplot.CanvasAxisLabelRenderer
            }
         }
     });




    });
</script>


